    <script>
        var parseInput = function (input) {
            var parsedInput = input.split(/\s+/);
            return parsedInput;
        };

        // puts textarea values onto the stack
        var parseStack = function (taval) {
            var ds0 = [];
            var ds = [];
            ds0 = taval.split("\n").reverse();
            for (i = 0; i < ds0.length; i++) {
                if (ds0[i].trim() != "") {
                    ds.push(ds0[i]);
                } 
            }
            return ds;
        };
        
